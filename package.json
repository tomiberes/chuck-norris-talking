{
  "name": "chuck-norris-talking",
  "description": "Learning Svelte",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "tailwind:dev": "postcss static/main.css -o static/global.css -w",
    "tailwind:build": "postcss static/main.css -o static/global.css",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "lint:fix": "eslint --fix \"{,!(node_modules|public)/**/}*.{js,svelte}\"",
    "lint:format": "prettier --write \"{,!(node_modules|public)/**/}*.{js,svelte}\"",
    "test": "run-p --race dev cy:run"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "ky-universal": "^0.8.1",
    "lodash": "^4.17.15",
    "polka": "next",
    "sirv": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "autoprefixer": "^9.8.4",
    "babel-eslint": "^10.1.0",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "eslint": "^7.4.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "prettier": "^2.0.5",
    "prettier-plugin-svelte": "^1.1.0",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^5.3.0",
    "sapper": "^0.27.0",
    "svelte": "^3.0.0",
    "tailwindcss": "^1.4.6"
  },
  "peerDependencies": {
    "ky": "^0.20.0"
  }
}
